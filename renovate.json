{
  "enabled": true,
  "major": {
    "enabled": true
  },
  "ignorePaths": [
    "**/node_modules/**"
  ],
  "packageFiles": [
    "package.json"
  ],
  "automerge": true,
  "rangeStrategy": "bump",
  "packageRules": [
    {
      "packagePatterns": [
        "*"
      ],
      "rangeStrategy": "bump"
    }
  ],
  "separateMajorMinor": true,
  "separateMultipleMajor": true,
  "rebaseStalePrs": true,
  "prCreation": "immediate",
  "prHourlyLimit": 0,
  "encrypted": {
    "npmToken": ""
  },
  "npmrc": "registry=https://npm.pkg.github.com/dagonmetric\n//npm.pkg.github.com/:_authToken=${NPM_TOKEN}\npackage-lock=false"
}
